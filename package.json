{
  "name": "oxalate-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/charts": "^2.1.0",
    "@ant-design/icons": "^5.3.7",
    "@ckeditor/ckeditor5-basic-styles": "^41.2.0",
    "@ckeditor/ckeditor5-build-classic": "^41.2.0",
    "@ckeditor/ckeditor5-essentials": "^41.2.0",
    "@ckeditor/ckeditor5-paragraph": "^41.2.0",
    "@ckeditor/ckeditor5-react": "^6.3.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "@types/react-highlight-words": "^0.16.7",
    "@vitejs/plugin-react": "^4.2.1",
    "antd": "^5.17.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.3",
    "env-cmd": "^10.1.0",
    "i18next": "^23.11.4",
    "i18next-http-backend": "^2.5.1",
    "qs": "^6.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-highlight-words": "^0.20.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.23.1",
    "translation-check": "^1.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^3.5.1"
  },
  "scripts": {
    "start": "env-cmd -f .env.local vite",
    "build:test": "tsc && vite build",
    "build:stage": "tsc && env-cmd -f .env.stage vite build",
    "build:production": "tsc && env-cmd -f .env.production vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage .",
    "test:debug": "jest --inspect-brk --runInBand --no-cache",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}
