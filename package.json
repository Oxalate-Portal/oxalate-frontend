{
  "name" : "oxalate-frontend",
  "version" : "0.9.0",
  "description" : "Oxalate Portal frontend",
  "author" : "Paul-Erik Törrönen",
  "homepage" : "https://oxalate.io",
  "license" : "GPL-2.0",
  "private" : true,
  "scripts" : {
    "prebuild" : "node generateBuildInfo.js",
    "prestart" : "node generateBuildInfo.js",
    "start" : "env-cmd -f .env.local vite",
    "build:test" : "npm run prebuild && vite build",
    "build:stage" : "npm run prebuild && env-cmd -f .env.stage vite build",
    "build:production" : "npm run prebuild && env-cmd -f .env.production vite build",
    "test" : "jest",
    "test:watch" : "jest --watch",
    "test:coverage" : "jest --coverage .",
    "test:debug" : "jest --inspect-brk --runInBand --no-cache",
    "preview" : "vite preview"
  },
  "eslintConfig" : {
    "extends" : [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist" : {
    "production" : [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development" : [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies" : {
    "@ant-design/charts" : "^2.6.2",
    "@ant-design/icons" : "^6.0.0",
    "@ant-design/v5-patch-for-react-19" : "^1.0.3",
    "@ckeditor/ckeditor5-react" : "^11.0.0",
    "@wojtekmaj/react-recaptcha-v3" : "^0.1.4",
    "antd" : "^5.27.1",
    "axios" : "^1.11.0",
    "ckeditor5" : "^46.0.2",
    "dayjs" : "^1.11.10",
    "dompurify" : "^3.2.6",
    "env-cmd" : "^11.0.0",
    "i18next" : "^25.4.2",
    "i18next-http-backend" : "^3.0.2",
    "prop-types" : "^15.8.1",
    "qs" : "^6.14.0",
    "react" : "^19.1.1",
    "react-csv" : "^2.2.2",
    "react-dom" : "^19.1.1",
    "react-highlight-words" : "^0.21.0",
    "react-i18next" : "^15.7.2",
    "react-router-dom" : "^7.8.2",
    "web-vitals" : "^5.1.0"
  },
  "devDependencies" : {
    "@babel/plugin-proposal-private-property-in-object" : "^7.21.11",
    "@testing-library/jest-dom" : "^6.8.0",
    "@testing-library/react" : "^16.3.0",
    "@testing-library/user-event" : "^14.6.1",
    "@types/dompurify" : "^3.2.0",
    "@types/jest" : "^30.0.0",
    "@types/node" : "^24.3.0",
    "@types/react" : "^19.1.11",
    "@types/react-csv" : "^1.1.10",
    "@types/react-dom" : "^19.1.7",
    "@types/react-highlight-words" : "^0.20.0",
    "@vitejs/plugin-react" : "^5.0.1",
    "husky" : "^9.1.7",
    "jest" : "^30.0.5",
    "jest-environment-jsdom" : "^30.0.5",
    "translation-check" : "^1.1.1",
    "ts-jest" : "^29.4.1",
    "typescript" : "^5.9.2",
    "vite" : "^7.1.3",
    "vite-plugin-svgr" : "^4.5.0",
    "vite-tsconfig-paths" : "^5.1.3"
  },
  "jest" : {
    "preset" : "ts-jest/presets/js-with-ts",
    "testEnvironment" : "jest-environment-jsdom",
    "moduleNameMapper" : {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$" : "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$" : "<rootDir>/__mocks__/styleMock.js"
    },
    "modulePaths" : [
      "<rootDir>/src"
    ]
  },
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/Oxalate-Portal/oxalate-frontend.git"
  }
}
